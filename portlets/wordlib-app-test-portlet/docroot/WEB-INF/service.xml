<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.liferay.docs.wordlibapp">
	<author>mike</author>
	<namespace>WLA</namespace>
    <entity name="WordlibData" local-service="true">
        <column name="wordlibId" type="long" primary="true"></column>
        <column name="groupId" type="long"></column>
        <column name="userId" type="long"></column>
        <column name="companyId" type="long"></column>
        <column name="imageId" type="long"></column>
        <column name="name" type="String"></column>
        <column name="person" type="String"></column>
        <column name="place" type="String"></column>
        <column name="adjective" type="String"></column>
        <column name="animal" type="String"></column>
        <column name="celebrity" type="String"></column>
        <column name="verb" type="String"></column>
        <column name="adverb" type="String"></column>
        <finder name="WordlibId" return-type="Collection">
            <finder-column name="wordlibId"></finder-column>
        </finder>
    </entity>
    <exceptions>
        <exception>WordlibName</exception>
        <exception>WordlibPerson</exception>
        <exception>WordlibPlace</exception>
        <exception>WordlibAdjective</exception>
        <exception>WordlibAnimal</exception>
        <exception>WordlibCelebrity</exception>
        <exception>WordlibVerb</exception>
        <exception>WordlibAdverb</exception>
    </exceptions>
</service-builder>